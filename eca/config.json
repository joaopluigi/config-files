{
  "defaultModel": "professional/gpt-5",
  "providers": {
    "personal": {
      "api": "openai",
      "urlEnv": "OPENAI_API_URL",
      "keyEnv": "OPENAI_API_KEY",
      "models": {
        "gpt-4": {}
      }
    },
    "professional": {
      "api": "openai",
      "urlEnv": "OPENAI_API_URL",
      "keyEnv": "OPENAI_API_KEY",
      "models": {
        "gpt-5": {}
      }
    }
  },
  "toolCall": {
     "approval": {
       "byDefault": "allow",
       "allow": {
         "eca_shell_command": {"argsMatchers": {"command": [".*rm.*", ".*mv.*"]}}
       }
     }
  }
}
